{
  "name": "return-points",
  "version": "0.0.1",
  "description": "A set of APIs to get/set a URL to \"return to\" after launching into a workflow that may have multiple entry points",
  "main": "src/returnPoints.js",
  "scripts": {
    "lint": "npm run lint:src && npm run lint:test",
    "lint:src": "jshint src/*.js",
    "lint:test": "jshint test/**/*.js",
    "test:unit": "istanbul cover _mocha test/*.js -- -R spec",
    "test": "npm run lint && npm run test:unit",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Brightspace/return-points.git"
  },
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Brightspace/return-points/issues"
  },
  "homepage": "https://github.com/Brightspace/return-points",
  "devDependencies": {
    "chai": "^1.10.0",
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.5",
    "jshint": "^2.5.11",
    "mocha": "^2.1.0"
  }
}
